<div class="row">
	<div class="col-md-4">
	</div>
	<div class="col-md-4">
		<table class="table table-reflow">
			<thead>
				<tr>
					<th>#</th>
					<th>Name</th>
					<th>Status</th>
					<th></th>
				</tr>
			</thead>
			<tbody>
				<%if (list.names.length > 0) { %>
					<% for(var i=0; i<list.names.length; i++) { %>
						<tr>
							<th scope="row"><%= i+1 %></th>
							<td>
								<%= list.names[i].full_name %>
							</td>
							<td>
								<%= list.names[i].status %>
							</td>
							<td>
								<button value="<%= list.names[i].player_id %>" fn="<%= list.names[i].full_name %>" type="button" class="removePlayer btn btn-xs btn-block btn-danger" action="remove">
									REMOVE
								</button>
							</td>
						</tr>
					<% } %>
				<% } %>
			</tbody>
		</table>
	</div>
	<div class="col-md-4">
	</div>
</div>
